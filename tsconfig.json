{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2021",
    "sourceMap": true,
    "allowJs": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "outDir": "./dist",
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "#managers/*": ["classes/managers/*"],
      "#models/*": ["models/*"],
      "#structs/*": ["classes/structs/*"],
      "#/*": ["*"]
    }
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}